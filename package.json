{
	"type": "module",
	"name": "my-webpack-project",
	"sideEffects": [
		"src/**/*.css"
	],
	"version": "1.0.0",
	"description": "My webpack project",
	"private": true,
	"dependencies": {},
	"devDependencies": {
		"@babel/core": "^7.18.9",
		"@babel/eslint-parser": "^7.18.9",
		"@babel/preset-env": "^7.18.9",
		"babel-loader": "^8.2.5",
		"css-loader": "^6.7.1",
		"eslint": "^8.20.0",
		"eslint-config-prettier": "^8.5.0",
		"html-loader": "^4.1.0",
		"html-webpack-plugin": "^5.5.0",
		"husky": "^4.3.8",
		"image-webpack-loader": "^8.1.0",
		"lint-staged": "^13.0.3",
		"mini-css-extract-plugin": "^2.6.1",
		"prettier": "^2.7.1",
		"stylelint": "^14.9.1",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-rational-order": "^0.1.2",
		"stylelint-config-standard": "^26.0.0",
		"stylelint-order": "^5.0.0",
		"terser-webpack-plugin": "^5.3.3",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.9.3"
	},
	"scripts": {
		"lint-css": "stylelint",
		"lint-css:fix": "stylelint --fix",
		"lint": "eslint --cache",
		"lint:fix": "eslint --fix",
		"format": "prettier --check",
		"format:fix": "prettier --write",
		"build": "webpack --mode=development",
		"build:prod": "webpack --mode=production --node-env=production",
		"start": "webpack-dev-server --mode=development --hot",
		"start:prod": "webpack-dev-server --mode=production --node-env=production",
		"report": "webpack --profile --json > stats.json && npx webpack-bundle-analyzer ./stats.json",
		"report:prod": "webpack --mode=production --node-env=production --profile --json > stats.json && npx webpack-bundle-analyzer ./stats.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pandamorda/Cooperation.git"
	},
